@using EGH01.Core;
@using EGH01.Models.EGH;
@using EGH01DB.Primitives;
@using EGH01DB.Types;
@model EGH01DB.CCOContext


@{
    ViewBag.Title = "PetrochemicalType";
    Layout = "~/Views/Shared/EGHLayout.cshtml";

    Menu head = new Menu(
                         new Menu.MenuItem("Создать", "PetrochemicalType.Create", true),
                         new Menu.MenuItem("Excel-формат", "PetrochemicalType.Excel", true)
                       );

    List<PetrochemicalType> list = new List<PetrochemicalType>();
    if (@Model != null) { Helper.GetListPetrochemicalType(@Model, ref list); };


}


<div class="box main">
    <div class="boxdata">
        <div class="row">
            <div class="col-xs-4"><h3>Химический состав нефтей</h3></div>
            <div class="col-xs-8">&ndash; электронный справочник по химическому составу нефти и нефтепродуктов.</div>
        </div>
        <div class="box main">
            <div class="boxdata">
                <div class="row">
                    <div class="col-xs-4"><h3>Категория нефтепродукта</h3></div>
                    <div class="col-xs-8">&ndash; электронный справочник по категориям нефтепродуктов.</div>
                </div>

            </div>
        </div>
    </div>
</div>
@Html.Partial("_TypeHeadMenu", new TypeViewMenu { Controller = "EGHCCO", Action = "PetrochemicalType", Menu = head })
@Html.Partial("_PetrochemicalTypeList", list)

